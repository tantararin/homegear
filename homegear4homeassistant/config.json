{
  "name": "Homegear4Homeassistant",
  "version": "1.0",
  "slug": "homegear4homeassistant",
  "description": "Homegear as a Hassio add-on",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "services",
  "boot": "auto",
  "kernel_modules": true,
  "uart": true,
  "gpio": true,
  "udev": true,
  "webui": "http://[HOST]:[PORT:2001]/admin",
  "apparmor": false,
  "full_access": true,
  "devices": ["/dev/ttyAMA0", "/dev/ttyACM0"],
  "options": {
  },
  "map": ["config:rw", "share:rw", "ssl:rw"],
  "ports": {
    "2001/tcp": 2001,
    "2002/tcp": 2002,
    "2003/tcp": 2003
  },
  "environment": {  },
  "schema": {  }
}
